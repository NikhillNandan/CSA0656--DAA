#include <stdio.h>
int fibonacci(int n) {
    if (n <= 0) return 0;
    else if (n == 1) return 1;
    else return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n_terms = 10;
    printf("K.NIKHILL NANDAN-192211750\n");
    printf("Fibonacci Series: ");
    for (int i = 0; i < n_terms; i++) {
        printf("%d ", fibonacci(i));
    }
    return 0;
